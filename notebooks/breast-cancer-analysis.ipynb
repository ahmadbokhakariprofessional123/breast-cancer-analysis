import pandas as pd
from ucimlrepo import fetch_ucirepo 

# Fetch the dataset
breast_cancer_wisconsin_diagnostic = fetch_ucirepo(id=17) 

# Extract data
X = breast_cancer_wisconsin_diagnostic.data.features
y = breast_cancer_wisconsin_diagnostic.data.targets

# Convert features (X) and targets (y) to pandas DataFrame
features_df = pd.DataFrame(X, columns=breast_cancer_wisconsin_diagnostic.variables.features.names)
target_df = pd.DataFrame(y, columns=['target'])  # Assuming 'target' as the column name for y

# Combine features and target into a single DataFrame
df = pd.concat([features_df, target_df], axis=1)

# Display the first few rows of the DataFrame
print(df.head())
